public class ListagemDeClientes {
    public static void main(String[] args) {
        System.out.println(">>> Listagem dos Clientes");
        System.out.println("_______________________________________");
        System.out.println("Nome = Fulano");
        System.out.println("Data de Nascimento = 1980");
        System.out.println("_______________________________________");
        System.out.println("Nome = Sicrano");
        System.out.println("Data de Nascimento = 2000");

    }
}
